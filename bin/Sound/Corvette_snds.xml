<?xml version="1.0" encoding="utf-8"?>
<protounitsounddef>
  <protounit name="Corvette">
    <soundtype name="Select">
      <soundset name="CorvetteSelect">
      </soundset>
    </soundtype>
    <soundtype name="Death">
      <soundset name="ShipDeath">
      </soundset>
    </soundtype>
    <soundtype name="Creation">
      <soundset name="ShipBirth">
      </soundset>
    </soundtype>
    <soundtype name="Acknowledge">
      <targetlogic>
        <choice name="default">
          <soundset name="CorvetteAcknowledge">
          </soundset>
        </choice>
        <choice name="enemy">
          <soundset name="CorvetteAttack">
          </soundset>
        </choice>
      </targetlogic>
    </soundtype>
    <soundtype name="Exists">
      <soundset name="AmbienceShip">
      </soundset>
    </soundtype>
  </protounit>
</protounitsounddef>